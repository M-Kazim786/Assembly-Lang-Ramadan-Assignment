;Input any letter and then show it 10 times separated with = sign.

.model small
.stack 100h
.data  
   msg1 db 'Enter a letter$'    
   msg2 db '=$'   
   
.code
main proc
      mov ax,@data
      mov ds,ax
      
      lea dx,msg1
      mov ah,9
      int 21h
      
      mov ah,1
      int 21h 

      mov cx,9
      mov bl,al     
      L1:
     
      mov dl,bl
      mov ah,2
      int 21h     
      lea dx,msg2
      mov ah,9
      int 21h 
       
      loop L1   
      mov dl,bl
      mov ah,2
      int 21h 
      
      mov ah,4ch
      int 21h
main endp
end main
